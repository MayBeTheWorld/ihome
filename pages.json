{
  "easycom": {
    "autoscan": true,
    // 注意一定要放在custom里，否则无效，https://ask.dcloud.net.cn/question/131175
    "custom": {
      "^u-(.*)": "uview-plus/components/u-$1/u-$1.vue",
      "nut-(.*)?-(.*)": "@/uni_modules/sky-nutui/components/sky-nutui/packages/__VUE/$1$2/index.vue",
      "nut-(.*)": "@/uni_modules/sky-nutui/components/sky-nutui/packages/__VUE/$1/index.vue"
    }
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页",
        "app-plus": {
          "titleNView": false
        }
      }
    },
    {
      "path": "pages/VR/VR",
      "style": {
        "navigationBarTitleText": "VR",
        "app-plus": {
          "titleNView": false
        }
      }
    },
    {
      "path": "components/scan/scanReturn",
      "style": {
      }
    },
    {
      "path": "pages/news/news",
      "style": {
        "navigationBarTitleText": "消息",
        "app-plus": {
          "titleNView": false
        }
      }
    },
    {
      "path": "pages/main/main",
      "style": {
        "navigationBarTitleText": "我的",
        "app-plus": {
          "titleNView": false
        }
      }
    },
    {
      "path": "pages/subpages/login/testLogin",
      "style": {
        "navigationBarTitleText": "测试登录",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/subpages/login/login",
      "style": {
        "navigationBarTitleText": "登录",
        "enablePullDownRefresh": false
      }
    }
  ],
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#32B0B5",
    "list": [
      {
        "text": "首页",
        "pagePath": "pages/index/index"
      },
      {
        "text": "VR",
        "pagePath": "pages/VR/VR"
      },
      {
        "text": "消息",
        "pagePath": "pages/news/news"
      },
      {
        "text": "我的",
        "pagePath": "pages/main/main"
      }
    ]
  },
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "爱佳装",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
